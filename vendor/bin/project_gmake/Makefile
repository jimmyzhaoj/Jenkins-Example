.NOTPARALLEL:

ifndef config
  config=debug_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x64)
  HelloWorld_config = debug_x64
endif
ifeq ($(config),debug_x32)
  HelloWorld_config = debug_x32
endif
ifeq ($(config),release_x64)
  HelloWorld_config = release_x64
endif
ifeq ($(config),release_x32)
  HelloWorld_config = release_x32
endif

PROJECTS := HelloWorld

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

HelloWorld:
ifneq (,$(HelloWorld_config))
	@echo "==== Building HelloWorld ($(HelloWorld_config)) ===="
	@${MAKE} --no-print-directory -C . -f HelloWorld.make config=$(HelloWorld_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f HelloWorld.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x64"
	@echo "  debug_x32"
	@echo "  release_x64"
	@echo "  release_x32"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   HelloWorld"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"